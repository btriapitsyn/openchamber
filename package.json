{
  "name": "opencode-webui",
  "version": "1.0.0",
  "description": "Web interface companion for OpenCode AI coding agent",
  "type": "module",
  "bin": {
    "opencode-webui": "./bin/cli.js"
  },
  "main": "server/index.js",
  "files": [
    "bin/",
    "server/",
    "dist/",
    "fix-deprecation.js"
  ],
  "engines": {
    "node": ">=16.0.0"
  },
  "keywords": [
    "opencode",
    "ai",
    "coding",
    "webui",
    "cli"
  ],
  "author": "fedaykin.dev",
  "license": "MIT",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "build:package": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "test:package": "npm run build:package && npm pack",
    "start": "node server/index.js",
    "cli": "node bin/cli.js",
    "postinstall": "node fix-deprecation.js",
    "convert-theme": "tsx scripts/convert-theme.ts"
  },
  "dependencies": {
    "@ibm/plex": "^6.4.1",
    "@opencode-ai/sdk": "^0.7.1",
    "@phosphor-icons/react": "^2.1.10",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-toggle": "^1.1.10",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@types/react-syntax-highlighter": "^15.5.13",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "express": "^5.1.0",
    "flowtoken": "^1.0.40",
    "http-proxy-middleware": "^3.0.5",
    "next-themes": "^0.4.6",
    "phosphor-icons": "^1.4.2",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-markdown": "^10.1.0",
    "react-syntax-highlighter": "^15.6.6",
    "remark-gfm": "^4.0.1",
